@model IEnumerable<SettingVM>

@{
    ViewData["Title"] = "Index";
}

<div class="page-header d-print-none mb-5">
    <div class="container-xl">
        <div class="row g-2 align-items-center">
            <div class="col">
                <!-- Page pre-title -->
                <h2 class="page-title">
                    Settings
                </h2>
            </div>
        </div>
    </div>
</div>
<section id="setting-area">
    <div class="card">
        <div class="card-body">
            <div id="table-default" class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th><button class="table-sort" data-sort="sort-name">Key</button></th>
                            <th><button class="table-sort" data-sort="sort-name">Value</button></th>
                            <th>Operations</th>
                        </tr>
                    </thead>
                    <tbody class="table-tbody">
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td class="sort-name">@item.Key</td>
                                <td class="sort-name">@item.Value</td>
                                <td>
                                    <a class="btn btn-warning btn-icon" asp-action="Edit" asp-route-id="@item.Id">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="icon icon-tabler icons-tabler-outline icon-tabler-pencil"><path stroke="none" d="M0 0h24v24H0z" fill="none" /><path d="M4 20h4l10.5 -10.5a2.828 2.828 0 1 0 -4 -4l-10.5 10.5v4" /><path d="M13.5 6.5l4 4" /></svg>
                                    </a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>

